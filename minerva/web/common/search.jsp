<div id="search">
  <h3>Search</h3>
  <script type="text/javascript" src="js/search.js"></script>
  <form id="search_form" action="javascript:search();">
    <span id="search_label">
      <label for="q">Search the site</label>
    </span>
    <span id="search_input">
      <input id="q" type="text" name="q" value=""/>
    </span>
    <label class="invisible" for="search_submit">Submit</label>
    <input id="search_submit" type="submit" value="Search" title="Search"/>
  </form>
</div> <!-- search -->
								
